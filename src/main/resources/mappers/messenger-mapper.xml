<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="Messenger">
	<select id="getAllMembers" resultType="com.kedu.project.dto.MemberDTO">
   SELECT * FROM Member
</select>

<update id="updateWorkStatus" parameterType="map">
    UPDATE member
    SET work_status = #{work_status}
    WHERE id = #{id}
</update>

</mapper>